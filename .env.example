# =============================================================================
# .env.example — Next.js / TS — Referenz für alle Env-Variablen.
# Next lädt .env.development (dev) und .env.production (build/start) automatisch. Optional: Copy nach .env und Werte anpassen.
# Provider-API-Keys (Finnhub, FRED, etc.) nur in go-backend/.env.
# =============================================================================

# ---------- CREDENTIALS (nicht committen; Prod: openssl rand -base64 32, siehe AGENTS.md) ----------
AUTH_SECRET=
NEXTAUTH_SECRET=
AUTH_KG_FALLBACK_KEY=
NEXTAUTH_ADMIN_USER=admin
NEXTAUTH_ADMIN_PASSWORD=change-me
NEXTAUTH_ADMIN_ROLE=admin

# ---------- Config ----------
DATABASE_URL="file:./dev.db"

NEXTAUTH_URL=http://localhost:3000
NEXT_PUBLIC_ENABLE_AUTH=false
CORS_ALLOWED_ORIGINS=http://localhost:3000
NEXT_PUBLIC_ENABLE_REMOTE_PERSISTENCE=false

AUTH_PASSKEY_SCAFFOLD_ENABLED=false
AUTH_STACK_BYPASS=true
NEXT_PUBLIC_AUTH_STACK_BYPASS=true
ALLOW_PROD_AUTH_STACK_BYPASS=false
NEXT_PUBLIC_AUTH_BYPASS_ROLE=admin
AUTH_BYPASS_ROLE=admin
# Optional (Prod): gleiche Werte wie Go AUTH_JWT_ISSUER/AUTH_JWT_AUDIENCE für strenge JWT-Prüfung
AUTH_JWT_ISSUER=
AUTH_JWT_AUDIENCE=
AUTH_PASSKEY_PROVIDER_ENABLED=true
NEXT_PUBLIC_AUTH_PASSKEY_PROVIDER_ENABLED=true
PAGE_SECURITY_HEADERS_ENABLED=false
PAGE_CSP_MODE=off
PAGE_CSP_POLICY=default-src 'self'; script-src 'self' 'unsafe-inline' 'unsafe-eval'; style-src 'self' 'unsafe-inline'
AUTH_WEBAUTHN_RP_NAME=TradeView Fusion
AUTH_WEBAUTHN_RP_ID=localhost
AUTH_WEBAUTHN_ORIGINS=http://localhost:3000
AUTH_DEFAULT_ROLE=viewer
AUTH_KG_FALLBACK_ROTATION_TAG=v1

MARKET_STREAM_LEGACY_FALLBACK_ENABLED=true
MARKET_STREAM_QUOTES_LEGACY_FALLBACK_ENABLED=true
ALLOW_PROD_MARKET_STREAM_LEGACY_FALLBACK=false

RUST_OHLCV_CACHE_ENABLED=true
RUST_OHLCV_CACHE_PATH=python-backend/.cache/rust-ohlcv-cache.redb

GEOPOLITICAL_INGEST_SHADOW_COMPARE=
GEOPOLITICAL_INGEST_HARD_MODE=next-proxy
GEOPOLITICAL_INGEST_SOFT_MODE=next-proxy
GEOPOLITICAL_ADMIN_SEED_MODE=next-proxy

NEXT_PUBLIC_ENABLE_GEOPOLITICAL_MAP=true
GEOPOLITICAL_CANDIDATE_MODE=true
GEOPOLITICAL_HARD_SIGNAL_MODE=true
GEOPOLITICAL_POLL_INTERVAL_MS=600000
GEOPOLITICAL_CANDIDATE_TTL_HOURS=72
GEOPOLITICAL_MAX_CANDIDATES_PER_RUN=100
GEOPOLITICAL_MAX_PROVIDER_CALLS_PER_RUN=100
ENABLE_ACLED_THRESHOLD_INGEST=false
ACLED_THRESHOLD_LOOKBACK_DAYS=3
ACLED_THRESHOLD_PAGE_SIZE=120
ACLED_THRESHOLD_MIN_SEVERITY=4
ACLED_THRESHOLD_EVENT_COUNT=5
ACLED_THRESHOLD_FATALITIES=0
NEWS_HTTP_TIMEOUT_MS=4000
GEOPOLITICAL_SOFT_SIGNAL_ENABLED=false
GEOPOLITICAL_SOFT_SIGNAL_URL=http://127.0.0.1:8091
GEOPOLITICAL_SOFT_SIGNAL_TIMEOUT_MS=8000
GEOPOLITICAL_SOFT_SIGNAL_MAX_CANDIDATES=6
SOFT_SIGNAL_CHRONICLE_POC_ENABLED=false
SOFT_SIGNAL_SCOUT_POC_ENABLED=false
SOFT_SIGNAL_FINGPT_POC_ENABLED=false
CRISISWATCH_RSS_URL=https://www.crisisgroup.org/rss-0
CRISISWATCH_CACHE_TTL_MS=300000
CRISISWATCH_CACHE_PERSIST_PATH=
GO_GATEWAY_BASE_URL=http://127.0.0.1:9060
GEOPOLITICAL_ACLED_CACHE_MS=60000
GEOPOLITICAL_CONTEXT_CACHE_MS=300000
GEOPOLITICAL_GAMETHEORY_CACHE_MS=120000
GEOPOLITICAL_GAMETHEORY_URL=http://127.0.0.1:8091
GEOPOLITICAL_GAMETHEORY_TIMEOUT_MS=5000
ENABLE_OFAC_INGEST=true
ENABLE_UK_SANCTIONS_INGEST=true
ENABLE_UN_SANCTIONS_INGEST=true
ENABLE_CENTRAL_BANK_CALENDAR_INGEST=true

INDICATOR_SERVICE_ENABLED=true
INDICATOR_SERVICE_URL=http://127.0.0.1:8092
INDICATOR_SERVICE_TIMEOUT_MS=8000

MACRO_INGEST_ENABLED=false
MACRO_INGEST_INTERVAL_MS=1800000
MACRO_INGEST_REQUEST_TIMEOUT_MS=8000
MACRO_INGEST_OUTPUT_DIR=go-backend/data/macro
GCT_BACKTEST_EXECUTOR_ENABLED=false
GCT_BACKTEST_GRPC_ADDRESS=127.0.0.1:9054
GCT_BACKTEST_INSECURE_TLS=false
GCT_BACKTEST_REQUEST_TIMEOUT_MS=8000
GCT_BACKTEST_POLL_INTERVAL_MS=750
GCT_BACKTEST_RUN_TIMEOUT_MS=300000
GCT_BACKTEST_REPORT_OUTPUT_DIR=go-backend/vendor-forks/gocryptotrader/backtester/results
