	"scripts": {
		"dev": "next dev -p 3000",
		"dev:with-python": "powershell -ExecutionPolicy Bypass -File ./python-backend/scripts/dev-with-python-services.ps1",
		"dev:full": "powershell -ExecutionPolicy Bypass -File ./scripts/dev-stack.ps1",
		"dev:full:gct": "powershell -ExecutionPolicy Bypass -File ./scripts/dev-stack.ps1",
