		"build": "next build",
		"start": "next start -p 3000",
		"lint": "biome check ./src",
		"lint:fix": "biome check --write ./src",
		"format": "biome format --write ./src",
		"db:push": "prisma db push",
		"db:generate": "prisma generate",
		"db:migrate": "prisma migrate dev",
		"db:reset": "prisma migrate reset",
		"test:e2e": "playwright test",
		"rust:fmt:check": "CARGO_TARGET_DIR=./python-backend/rust_core/target-local \"$HOME/.cargo/bin/cargo.exe\" fmt --manifest-path ./python-backend/rust_core/Cargo.toml -- --check",
		"rust:clippy": "CARGO_TARGET_DIR=./python-backend/rust_core/target-local \"$HOME/.cargo/bin/cargo.exe\" clippy --manifest-path ./python-backend/rust_core/Cargo.toml --all-targets --all-features -- -D warnings",
		"rust:test": "CARGO_TARGET_DIR=./python-backend/rust_core/target-local \"$HOME/.cargo/bin/cargo.exe\" test --manifest-path ./python-backend/rust_core/Cargo.toml --all-features",
		"rust:check": "CARGO_TARGET_DIR=./python-backend/rust_core/target-local \"$HOME/.cargo/bin/cargo.exe\" check --manifest-path ./python-backend/rust_core/Cargo.toml --all-targets --all-features",
		"rust:build": "CARGO_TARGET_DIR=./python-backend/rust_core/target-local \"$HOME/.cargo/bin/cargo.exe\" build --manifest-path ./python-backend/rust_core/Cargo.toml --release",
		"rust:lint": "bun run rust:fmt:check && bun run rust:clippy",
		"rust:ci": "bun run rust:fmt:check && bun run rust:clippy && bun run rust:test && bun run rust:check",
		"rust:py:build": "uv run maturin build -m ./python-backend/rust_core/Cargo.toml",
		"rust:py:develop": "uv run maturin develop -m ./python-backend/rust_core/Cargo.toml"
	},
